    "@tamagui/toast": "^1.81.4",
    "@tanstack/react-query": "^5.17.0",
    "burnt": "^0.12.2",
    "expo": "~49.0.11",
    "expo-build-properties": "~0.8.3",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
